<mvc:View
    controllerName="project1.controller.covid.List"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:grid="sap.ui.layout.cssgrid"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
 
>

    <Page
        id="CovidList"
        showHeader="false"
    >
    	<List
		id="myList"
		headerText="total cases{Latest>data/summary/total}"
		footerText="Latest Covid Count"
		
		
		items= "{Latest>/data/regional}"
		>
		<items>
			<StandardListItem
				title="{Latest>loc}"
				description="Total case {Latest>totalConfirmed}"
				info="{Latest>deaths}"/>
				
		</items>
	</List>
    

            </Page>


</mvc:View>
